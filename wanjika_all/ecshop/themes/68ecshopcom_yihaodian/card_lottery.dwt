	<script type="text/javascript" src="themes/68ecshopcom_yihaodian/js/jquery.easing.min.js"></script>
	<script>
		$(window).resize(function(){
			setwin();
		});
		function setwin(){
			var w = ($("body").width()-1202)/4-35;
			$(".leftwin").css("right",w);
		} 
		setwin();
		
		$(".more").on("click",function(){
			$(this).prev().toggleClass("h200");
			if($(this).text()=="关闭"){
				$(this).text("展开更多");
			}else{
				$(this).text("关闭");
			}
		});
		
		var in_lottery = false;
		
		$(function(){
			$("#startbtn").click(function()
			{
				lottery();
			});
			
			function lottery()
			{
				if (!in_lottery)
				{
					in_lottery = true;
					Ajax.call('card_lottery.php?act=lottery', '',lotteryResponse, 'GET', 'JSON');
				}
			}
			
			function lotteryResponse(obj)
			{
	            var a = obj.angle; //角度 
	            if (a > 0)
	            {
		            $("#startbtn").rotate({ 
		                duration:3000, //转动时间 
		                angle: 0, 
		                animateTo:1800+a, //转动角度 
		                easing: $.easing.easeOutSine, 
		                callback: function(){ 
		                    alert(obj.msg);
		                    in_lottery = false;
		                } 
		            }); 
	            }
	            else
	            {
	            	alert(obj.msg);
	            	in_lottery = false;
	            }
			}
		});
</script>
